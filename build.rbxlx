		assert(sync, "connect() must be called before _sendPatch()")

		if not changed then
			return
		end

		local payload: SyncPayload = {
			type = "patch",
			data = patch.diff(previousSnapshot, currentSnapshot, autoSerialize),
		}

		previousSnapshot = table.clone(currentSnapshot)
		changed = false

		sync(player, payload)
	end

	return self
end

