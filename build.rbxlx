local function debounce(callback, wait, options)
	if wait == nil then
		wait = 0
	end
	if options == nil then
		options = {}
	end
	local _binding = options
	local leading = _binding.leading
	if leading == nil then
		leading = false
	end
	local trailing = _binding.trailing
	if trailing == nil then
		trailing = true
