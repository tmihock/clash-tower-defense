            <string name="Source"><![CDATA[-- Compiled with roblox-ts v3.0.0
local TS = require(game:GetService("ReplicatedStorage"):WaitForChild("rbxts_include"):WaitForChild("RuntimeLib"))
local EnemyConfig = TS.import(script, game:GetService("ReplicatedStorage"), "TS", "config", "EnemyConfig").EnemyConfig
local ReplicatedStorage = TS.import(script, game:GetService("ReplicatedStorage"), "rbxts_include", "node_modules", "@rbxts", "services").ReplicatedStorage
local Signal = TS.import(script, game:GetService("ReplicatedStorage"), "rbxts_include", "node_modules", "@rbxts", "lemon-signal", "src").default
local Events = TS.import(script, game:GetService("ServerScriptService"), "TS", "networking").Events
local enemyFolder = ReplicatedStorage.Assets.Enemies
local Enemy
do
	Enemy = setmetatable({}, {
		__tostring = function()
			return "Enemy"
		end,
	})
	Enemy.__index = Enemy
	function Enemy.new(...)
		local self = setmetatable({}, Enemy)
		return self:constructor(...) or self
	end
	function Enemy:constructor(enemy, id)
		self.id = id
		self.timeSpawned = os.clock()
		self.position = Vector3.zero
		self.destroying = Signal.new()
		self.info = EnemyConfig[enemy]
		self.health = self.info.health
	end
	function Enemy:takeDamage(value)
		self:setHealth(self.health - value)
