			return function(_, Function)
				return self:Connect(Function)
			end
		elseif Index == "Wait" or Index == "wait" then
			return function()
				return self:Wait()
			end
		elseif Index == "Once" then
			return function(_, Function)
				return self:ConnectOnce(Function)
